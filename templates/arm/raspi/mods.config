
package genconfig

configuration conf {
	@Runlevel(0) include embox.arch.generic.arch
	@Runlevel(0) include embox.arch.arm.armlib.locore
	//@Runlevel(0) include embox.arch.arm.armlib.interrupt
	@Runlevel(0) include embox.arch.generic.interrupt_stub
	@Runlevel(0) include embox.arch.arm.armlib.context
	@Runlevel(0) include embox.arch.arm.libarch

	@Runlevel(0) include embox.arch.system(core_freq=70000000)
	@Runlevel(0) include embox.kernel.stack(stack_size=1536,alignment=4)

	@Runlevel(1) include embox.driver.interrupt.no_interrupts
	@Runlevel(1) include embox.driver.clock.cortexm_systick

	@Runlevel(1) include embox.driver.diag(impl="embox__driver__serial__raspi_uart")
	@Runlevel(1) include embox.driver.serial.raspi_uart

	//include embox.driver.gpio.TODO

	include embox.kernel.critical
	//include embox.kernel.irq
	include embox.kernel.spinlock(spin_debug=false)
	include embox.kernel.task.single
	include embox.kernel.task.resource.idesc_table(idesc_table_size=1)
	include embox.kernel.task.task_no_table

	include embox.driver.interrupt.no_interrupts
	include embox.kernel.no_irq
	include embox.kernel.irq_lock


	@Runlevel(1) include embox.kernel.timer.sys_timer(timer_quantity=2)
	@Runlevel(1) include embox.kernel.timer.strategy.list_timer
	@Runlevel(1) include embox.kernel.time.kernel_time
	@Runlevel(1) include embox.kernel.timer.itimer(itimer_quantity=0)
	include embox.kernel.timer.sleep_nosched

	include embox.kernel.thread.core(thread_pool_size=0)
	include embox.kernel.thread.signal_stub

	@Runlevel(1) include embox.kernel.lthread.lthread
	@Runlevel(2) include embox.kernel.sched.sched
	@Runlevel(2) include embox.kernel.sched.idle_light
	@Runlevel(2) include embox.kernel.sched.sched_ticker_stub
	@Runlevel(2) include embox.kernel.sched.strategy.priority_based


	/* tty requires */
	include embox.driver.serial.fsnode_none

	@Runlevel(2) include embox.cmd.shell
	include embox.init.setup_tty_diag
	@Runlevel(3) include embox.init.start_script(shell_name="diag_shell")

	include embox.cmd.help
	include embox.cmd.sys.version

	include embox.util.DList
	include embox.compat.libc.stdio.print(support_floating=0)
	include third_party.lib.libgcc_toolchain
}
